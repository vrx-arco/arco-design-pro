  "peerDependenciesMeta": {
    "unplugin-vue-components": {
      "optional": true
    }
  },
  "dependencies": {
    "@vrx-arco/css-render": "workspace:*",
    "@vrx-arco/icon": "workspace:*",
    "@vrx-arco/use": "workspace:*",
    "@vueuse/core": "^9.4.0",
    "vue-types": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "web-types": "web-types.json"
