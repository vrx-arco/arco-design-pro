      "import": "./resolver.mjs",
      "require": "./resolver.js",
      "types": "./resolver.d.ts"
    }
  },
  "files": [
    "dist/*",
    "arco-style-css.js",
    "arco-style-css.mjs",
    "arco-style.js",
    "arco-style.mjs",
    "web-types.json",
    "components.d.ts",
    "resolver.js",
    "resolver.mjs",
    "resolver.d.ts"
  ],
  "scripts": {
    "build": "shared && vite build && tsup-node"
  },
  "peerDependencies": {
    "@arco-design/web-vue": "^2.38.0",
