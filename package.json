    "@vill-v/tsconfig": "^1.0.1",
    "@vrx-arco/internal": "workspace:*",
    "@vrx/cp": "^0.2.3",
    "automd": "^0.3.8",
    "eslint": "^9.9.1",
    "knip": "^5.29.2",
    "less": "^4.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "taze": "^0.16.7",
    "tsup": "^8.2.4",
